<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:emulationEnvironmentTemplate xmlns:ns2="http://bwfla.bwl.de/common/datatypes">
    <ns2:id>win7_generic</ns2:id>
    <ns2:description>
        <ns2:title>Windows 7 (generic ide)</ns2:title>
        <ns2:os>win7</ns2:os>
    </ns2:description>
    <ns2:metaDataVersion>1</ns2:metaDataVersion>
    <ns2:arch>x86_64</ns2:arch>
    <ns2:model xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/>
    <ns2:emulator bean="Qemu"/>
    <ns2:ui_options>
        <ns2:html5>
            <ns2:pointer_lock>false</ns2:pointer_lock>
            <ns2:crt></ns2:crt>
        </ns2:html5>
        <ns2:input required="false">
            <ns2:clientKbdLayout>de</ns2:clientKbdLayout>
        </ns2:input>
    </ns2:ui_options>
    <ns2:operatingSystemId xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">Q6072277</ns2:operatingSystemId>
    <ns2:drive>
        <ns2:data>binding://main_hdd</ns2:data>
        <ns2:iface>ide</ns2:iface>
        <ns2:bus>0</ns2:bus>
        <ns2:unit>0</ns2:unit>
        <ns2:type>disk</ns2:type>
        <ns2:boot>true</ns2:boot>
        <ns2:plugged>true</ns2:plugged>
    </ns2:drive>
    <ns2:drive>
        <ns2:data></ns2:data>
        <ns2:iface>ide</ns2:iface>
        <ns2:bus>0</ns2:bus>
        <ns2:unit>1</ns2:unit>
        <ns2:type>cdrom</ns2:type>
        <ns2:filesystem>ISO</ns2:filesystem>
        <ns2:boot>false</ns2:boot>
        <ns2:plugged>true</ns2:plugged>
    </ns2:drive>
    <ns2:drive>
        <ns2:data></ns2:data>
        <ns2:iface>floppy</ns2:iface>
        <ns2:bus>0</ns2:bus>
        <ns2:unit>0</ns2:unit>
        <ns2:type>floppy</ns2:type>
        <ns2:filesystem>FAT12</ns2:filesystem>
        <ns2:boot>false</ns2:boot>
        <ns2:plugged>false</ns2:plugged>
    </ns2:drive>
    <ns2:nativeConfig>-usb -enable-kvm -usbdevice tablet -vga cirrus -soundhw sb16 -net nic,model=rtl8139 -net user -m 1500</ns2:nativeConfig>
    <ns2:imageGeneralization>
        <ns2:modificationScript>patch-win7.sh</ns2:modificationScript>
	<ns2:precondition>
		<ns2:fileSystem>ntfs</ns2:fileSystem>
        	<ns2:partitionLabel></ns2:partitionLabel>
	        <ns2:requiredFiles>
			<ns2:fileName>/Windows/System32/config/SYSTEM</ns2:fileName>			
                </ns2:requiredFiles>
	</ns2:precondition>
    </ns2:imageGeneralization>
</ns2:emulationEnvironmentTemplate>

